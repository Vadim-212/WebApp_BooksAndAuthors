@model IEnumerable<WebApplication1.Entity.UsersBooks>
<h2>@ViewBag.CurrentUserHistoryName</h2>

<table class="table">
    <thead>
        <th>Id</th>
        <th>User</th>
        <th>Book</th>
        <th>Issue Date</th>
        <th>Time</th>
        <th>Return Date</th>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>item.Id</td>
                <td>item.UserId</td>
                <td>item.BookId</td>
                <td>item.IssueDate</td>
                <td>item.Time</td>
                <td>item.ReturnDate</td>
            </tr>
        }
    </tbody>
</table>