<table class="table">
    <thead>
        <th>Id</th>
        <th>Name</th>
    </thead>
    <tbody>
        @foreach(var item in ViewBag.AuthorsList)// as List<WebApplication1.Entity.Authors>)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FirstName @item.LastName</td>
            </tr>
        }
    </tbody>
</table>